<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef Name="RGKRugBase" Abstract="True">
		<category>Building</category>
		<thingClass>Building</thingClass>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<selectable>true</selectable>
		<drawerType>MapMeshAndRealTime</drawerType>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<repairEffect>Repair</repairEffect>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<filthLeaving>Filth_RubbleBuilding</filthLeaving>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsFurniture</li>
		</thingCategories>
		<comps>
			<li>
				<compClass>CompQuality</compClass>
			</li>
			<!-- <li Class="CompProperties_RoomIdentifier"> -->
				<!-- <roomStat>Impressiveness</roomStat> -->
			<!-- </li> -->
		</comps>
		<designationCategory>OldStyleDeco</designationCategory>
		<tradeTags>
			<li>Furniture</li>
		</tradeTags>
	</ThingDef>
	
	<ThingDef ParentName="RGKRugBase">
		<defName>RugA_Small</defName>
		<label>Celtic Rug</label>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Things/Building/Rug/Rug1</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(3,3)</drawSize>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
		</graphicData>
		<building>    
			<isEdifice>false</isEdifice>
		</building>
		<altitudeLayer>FloorEmplacement</altitudeLayer>
		<passability>Standable</passability>
		<statBases>
			<MaxHitPoints>140</MaxHitPoints>
			<WorkToBuild>650</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>20</Beauty>
			<Cleanliness>0.5</Cleanliness>
			<Mass>10</Mass>
		</statBases>
		<size>(3,3)</size>
		<stuffCategories>
			<li>Leathery</li>
			<li>Fabric</li>
		</stuffCategories>
		<costStuffCount>90</costStuffCount>
		<description>It's a rug with Celtic designs.</description>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<rotatable>false</rotatable>
		<clearBuildingArea>false</clearBuildingArea>
		<saveCompressible>false</saveCompressible>
		<designationHotKey>Misc7</designationHotKey>
		<researchPrerequisites>
			<li>ComplexFurniture</li>
		</researchPrerequisites>
	</ThingDef>
